blue=$(tput setaf 4)
normal=$(tput sgr0)
echo 'files to add'
read files
for word in $files 
  do
    if [ $word == '.' ]
      then
      add .
    fi
    git add $word
    echo $word
  done
echo 'Commit Message'
read mesg
git commit -m $mesg 
printf " ${blue} Input Master user username and password\n ${normal}"
git push origin master
echo "Second User Name"
read $user2
printf " ${blue} Input Second user username and password\n ${normal}"
git push $user2 master
