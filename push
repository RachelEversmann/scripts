## $1 user2,commit message $2, user1 is origin
blue=$(tput setaf 4)
normal=$(tput sgr0)
echo 'files to add'
read files
echo $files
if [ $files != '.' ]
then
 for var in ${@:3}
  do
    git add $var
  done
else
  git add . 
fi
echo 'Commit Message'
read mesg
git commit -m $mesg 
printf " ${blue} Input Master user username and password\n ${normal}"
git push origin master
echo "Second User Name"
read $user2
printf " ${blue} Input Second user username and password\n ${normal}"
git push $user2 master
